xsetwacom --set "Wacom Cintiq 22HD Pad pad" Button 1 "key ctrl s"
xsetwacom --set "Wacom Cintiq 22HD Pad pad" Button 10 "key ctrl t"
xsetwacom --set "Wacom Cintiq 22HD Pad pad" Button 11 "key z"
xsetwacom --set "Wacom Cintiq 22HD Pad pad" Button 12 "key ctrl r"
xsetwacom --set "Wacom Cintiq 22HD Pad pad" Button 13 "key shift backspace"
